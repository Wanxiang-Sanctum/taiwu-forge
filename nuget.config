<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <add key="WanxiangSanctum" value="https://nuget.pkg.github.com/Wanxiang-Sanctum/index.json" />
        <add key="iplaylf2" value="https://nuget.pkg.github.com/iplaylf2/index.json" />
        <add key="local" value=".lf2.nupkg" />
    </packageSources>
    <packageSourceCredentials>
        <WanxiangSanctum>
            <add key="Username" value="%GITHUB_USERNAME%" />
            <add key="ClearTextPassword" value="%GITHUB_TOKEN%" />
        </WanxiangSanctum>
        <iplaylf2>
            <add key="Username" value="%GITHUB_USERNAME%" />
            <add key="ClearTextPassword" value="%GITHUB_TOKEN%" />
        </iplaylf2>
    </packageSourceCredentials>
    <disabledPackageSources>
        <add key="local" value="true" />
    </disabledPackageSources>
</configuration>